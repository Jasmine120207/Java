class Parent{
    static void display(){
       System.out.println("display method in parent class");
    }
}
class Child extends Parent{
    static void display(){
        System.out.println("display method in child class");
    }
}
class MethodHiding{
     public static void main(String[] args){
          Parent p=new Parent();
          p.display();
          Parent c=new Child();
          c.display();
     }
